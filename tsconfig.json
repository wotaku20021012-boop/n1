{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "types": [
      "node",
      "nuxt"
    ],
    "skipLibCheck": true
  },
  "include": [
    "nuxt.config.ts",
    "app.vue",
    "app/**/*.vue",
    "pages/**/*.vue",
    "components/**/*.vue",
    ".nuxt/**/*.d.ts",
    "env.d.ts"
  ]
}